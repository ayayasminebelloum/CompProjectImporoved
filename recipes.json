      "category": "Sweet",
      "time": "25 minutes",
      "ingredients": [
        {"name": "flour", "quantity": "2", "unit": "cups"},
        {"name": "sugar", "quantity": "1", "unit": "cup"},
        {"name": "butter", "quantity": "1/2", "unit": "cup"},
